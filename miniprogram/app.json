{
  "pages": [
    "page/component/index",
    "common/header/header",
    "pages/curriculum/curriculum",
    "pages/ppt/ppt"
  ],
  "usingComponents": {
    "common-header": "common/header/header"
  },
  "subpackages": [
    {
      "root": "packageComponent",
      "pages": [
        "pages/ppt/ppt",
        "pages/curriculum/curriculum",
        "pages/view/view/view"
      ]
    }
  ],
  "preloadRule": {
    "page/component/index": {
      "network": "all",
      "packages": [
        "packageComponent"
      ]
    }
  },
  "window": {
    "navigationBarTextStyle": "@navigationBarTextStyle",
    "navigationBarTitleText": "小助手",
    "navigationBarBackgroundColor": "@navigationBarBackgroundColor",
    "backgroundColor": "@backgroundColor"
  },
  "tabBar": {
    "color": "@tabBarColor",
    "selectedColor": "@tabBarSelectedColor",
    "borderStyle": "@tabBarBorderStyle",
    "backgroundColor": "@tabBarBackgroundColor",
    "list": [
      {
        "pagePath": "page/component/index",
        "iconPath": "@iconPathComponent",
        "selectedIconPath": "@selectedIconPathComponent",
        "text": "目录"
      },
      {
        "pagePath": "pages/curriculum/curriculum",
        "iconPath": "@iconPathComponent",
        "selectedIconPath": "@selectedIconPathComponent",
        "text": "待开发"
      },
      {
        "pagePath": "pages/ppt/ppt",
        "iconPath": "@iconPathAPI",
        "selectedIconPath": "@selectedIconPathAPI",
        "text": "待开发"
      }
    ]
  },
  "networkTimeout": {
    "request": 10000,
    "connectSocket": 10000,
    "uploadFile": 10000,
    "downloadFile": 10000
  },
  "workers": "workers",
  "debug": false,
  "style": "v2",
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  },
  "cloud": true,
  "sitemapLocation": "sitemap.json",
  "themeLocation": "demo.theme.json",
  "darkmode": false,
  "resizable": true,
  "useExtendedLib": {
    "weui": true
  },
  "frameset": true,
  "requiredBackgroundModes": [
    "audio",
    "location"
  ],
  "embeddedAppIdList": [
    "wxfdcee92a299bcaf1"
  ],
  "lazyCodeLoading": "requiredComponents"
}